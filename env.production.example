# Production Environment Variables
# Copy this file to .env.local and update with your production values

# Backend API URL (Update with your production backend URL)
NEXT_PUBLIC_API_BASE_URL=https://your-backend-domain.com

# Environment
NEXT_PUBLIC_ENVIRONMENT=production
NODE_ENV=production

# OpenAI API Key (Optional - if not using backend)
# OPENAI_API_KEY=your_production_openai_key_here

# Analytics (Optional)
# NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id

# Sentry (Optional - for error tracking)
# SENTRY_DSN=your_sentry_dsn
# NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn









